{
  "id": "pf1e-caravans",
  "title": "Pathfinder 1e - Caravans",
  "version": "1.1.0",
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "relationships": {
    "systems": [
      {
        "id": "pf1",
        "type": "system",
        "compatibility": {}
      }
    ],
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "11.0",
          "verified": "11.0"
        }
      }
    ]
  },
  "description": "Add a caravan sheet and caravan item types to the Pathfinder 1e system.",
  "authors": [
    {
      "name": "KitCat",
      "discord": "kitcat",
      "email": "katsulynx@gmail.com",
      "flags": {}
    }
  ],
  "esmodules": [
    "modules/pf1e-caravans.mjs"
  ],
  "styles": [
    "styles/pf1e-caravans.css"
  ],
  "packs": [
    {
      "name": "caravan-content",
      "label": "Caravan Content",
      "path": "packs/caravan-content",
      "type": "Item",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      },
      "system": "pf1",
      "flags": {}
    }
  ],
  "documentTypes": {
    "Actor": {
      "caravan": {
        "htmlFields": [
          "details.notes.value"
        ],
        "filePathFields": {
          "img": [
            "IMAGE"
          ]
        }
      }
    },
    "Item": {
      "equipment": {
        "htmlFields": [
          "description.value"
        ],
        "filePathFields": {
          "img": [
            "IMAGE"
          ]
        }
      },
      "wagon": {
        "htmlFields": [
          "description.value"
        ],
        "filePathFields": {
          "img": [
            "IMAGE"
          ]
        }
      },
      "traveler": {
        "htmlFields": [
          "description.value"
        ],
        "filePathFields": {
          "img": [
            "IMAGE"
          ]
        }
      },
      "feat": {
        "htmlFields": [
          "description.value"
        ],
        "filePathFields": {
          "img": [
            "IMAGE"
          ]
        }
      }
    }
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "de",
      "name": "German (Deutsch)",
      "path": "lang/de.json"
    }
  ],
  "readme": "README.md",
